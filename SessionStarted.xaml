<Window
    x:Class="YourTimeApp.SessionStarted"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:YourTimeApp"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="SessionStarted"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid ShowGridLines="True">
        <StackPanel Orientation="Vertical">
            <Label x:Name="MainLabel">Current Time Spent In Session</Label>
            <Button
                x:Name="MainButton"
                Width="100"
                HorizontalAlignment="Left"
                Click="MainButton_Click"
                Cursor="Hand">
                Click to start a session
            </Button>
            <Label x:Name="TaskTimer">Current Time Left For Task</Label>
            <Button
                x:Name="StartTaskButton"
                Width="100"
                HorizontalAlignment="Left"
                Click="StartTaskButton_Click"
                Cursor="Hand">
                Click to start a task
            </Button>
        </StackPanel>
    </Grid>

</Window>
